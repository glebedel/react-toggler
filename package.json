{
  "name": "react-toggler",
  "version": "1.0.0",
  "description": "a react component to create toggle buttons which modify parent's state",
  "main": "Toggler.js",
  "scripts": {
    "build": "babel ./jsx --out-dir ./",
    "preunittest": "babel tests/ --out-dir tests/",
    "unittest": "browserify -e tests/testToggler.js -o tests/public/build.js",
    "test": "jshint jsx/ --exclude node_modules",
    "prepublish": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glebedel/react-toggler.git"
  },
  "keywords": [
    "react-component",
    "react",
    "component",
    "toggler",
    "button"
  ],
  "author": "guillaume.lebedel@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glebedel/react-toggler/issues"
  },
  "homepage": "https://github.com/glebedel/react-toggler#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": "^0.14.5",
    "react-addons-css-transition-group": "^0.14.5"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "jshint": "^2.7.0",
    "react-dom": "^0.14.5"
  }
}
